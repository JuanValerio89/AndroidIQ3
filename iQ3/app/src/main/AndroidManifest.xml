<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    package="com.havr.iq3.arq.iq3">

    <application
        android:allowBackup="true"
        android:icon="@mipmap/logo_iq"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/logo_iq"
        android:debuggable="false"
        android:supportsRtl="true"
        android:theme="@style/AppTheme"
        tools:ignore="HardcodedDebugMode">
        <activity android:name=".MainActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name=".Seleccion" />
        <activity
            android:name=".Manual"
            android:label="@string/title_activity_manual"
            android:theme="@style/AppTheme.NoActionBar" />
        <activity android:name=".Perfiles" />
        <activity android:name=".Cotizaciones"></activity>
    </application>

</manifest>